<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="fretboard">
	<schema id="dev.bragefuglseth.Fretboard" path="/dev/bragefuglseth/Fretboard/">
	  <key name="window-width" type="i">
			<default>550</default>
		</key>
		<key name="window-height" type="i">
			<default>650</default>
		</key>
		<key name="is-maximized" type="b">
			<default>false</default>
		</key>
	  <key name="instrument" type="s">
			<default>'guitar-right-handed'</default>
	    <description>Can be either guitar-right-handed or guitar-left-handed.</description>
		</key>
	</schema>
</schemalist>
